  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "dev": "set \"NODE_ENV=development\" && tsnd --respawn --files src/app.ts",
    "test": "set \"NODE_ENV=test\" && mocha -r ts-node/register 'src/test/**/*.ts' --exit --timeout 5000",
    "swagger": "tsc && node ./dist/swagger-spec.js && cp src/swagger-output.json dist/swagger-output.json",
    "start": "node dist/app.js",
    "build": "tsc",
    "migrate": "tsc && npx sequelize db:migrate",
    "migrateundo": "tsc && npx sequelize db:migrate:undo:all",
    "seed": "tsc && npx sequelize db:seed:all",
    "seedundo": "tsc && npx sequelize db:seed:undo:all",
    "setupdb": "tsc && npx sequelize db:migrate && npx sequelize db:seed:all",
    "cleardb": "tsc && npx sequelize db:seed:undo:all && npx sequelize db:migrate:undo",
    "coverage":"nyc npm run test"
  },
